// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  // provider = "postgresql"
  // url      = env("DATABASE_URL")
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Selery {
  id            String  @id @default(uuid())
  row           Int
  jan           String?
  stock         Int?
  productNumber String?
  productName   String?
  color         String?
  size          String?
  nextTimeStock Int?
  nextTimeDate  String?
  createdAt      String?
}

